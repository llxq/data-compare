{
  "name": "@chendf/data-compare",
  "version": "1.2.0",
  "description": "data-compare是一个对比数据之间差异的一个轻量级库，类似于简化版本的git对比。也可以单纯用来对比两个数据之间的差异。",
  "main": "main.ts",
  "module": "main.ts",
  "publishConfig": {
    "main": "index.js",
    "module": "index.mjs"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/llxq/data-compare.git",
    "lint": "eslint --ext .js,.ts src --fix"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/llxq/data-compare/issues"
  },
  "homepage": "https://github.com/llxq/data-compare#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "commander": "^9.1.0",
    "eslint": "^8.10.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "rollup": "^2.68.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "shelljs": "^0.8.5",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  }
